@model List<RunRequest.Models.JobConfig>
@if (Model.Count == 0)
{


    <p>No Configuration tables found for this Job</p>
}
else
{
    <ul>
        @for (int i = 0; i < Model.Count(); i++)
        {
            <li><a href="@Model[i].url@Model[i].TableName" target="_blank">@Model[i].TableName</a></li>
        }
    </ul>

}


